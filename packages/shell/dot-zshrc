# vim: ft=zsh

# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"

plugins=(
  git
  macos
  taskwarrior
)

source /opt/homebrew/opt/spaceship/spaceship.zsh #brew install spaceship
source ~/.zsh-theme-settings
source ~/.exports
source ~/.aliases
source ~/.path
source ~/.functions
source ~/.extras 2> /dev/null

# this should be last
source ~/.oh-my-zsh/oh-my-zsh.sh
