# vim: ft=zsh

# Path to your oh-my-zsh installation.
export ZSH="$HOME/.oh-my-zsh"

plugins=(
  git
  osx
  taskwarrior
)


# some stuff for pyenv
if command -v pyenv 1>/dev/null 2>&1; then
    eval "$(pyenv init -)"
fi


source ~/.oh-my-zsh/oh-my-zsh.sh
source ~/.zsh-theme-settings
source ~/.exports
source ~/.aliases
source ~/.path
source ~/.functions
source ~/.extras
